{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-covid-demo-adf"
		},
		"ls_blob_coviddemostrgacct_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://devcoviddemostrgacct.blob.core.windows.net/"
		},
		"ls_datalake_coviddemodl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devcoviddemodl.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dev-covid-demo-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_coviddemostrgacct_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('ls_blob_coviddemostrgacct_properties_typeProperties_serviceEndpoint')]"
					},
					"ls_datalake_coviddemodl_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_coviddemodl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}